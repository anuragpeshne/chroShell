{
  "manifest_version": 2,

  "name": "chroShell",
  "description": "Client for shServer",
  "version": "1.0",

  "browser_action": {
    "default_icon": "terminalIcon.png",
    "default_popup": "popup.html",
    "default_title": "ChroShell"
  },
  "permissions": [
    "activeTab",
    "alarms",
    "http://localhost/*",
    "clipboardRead",
    "downloads"
  ]
}
